{
    "name": "sample-shell",
    "version": "0.0.1",
    "scripts": {
      "ng": "ng",
      "gv": "genversion --esm --semi src/lib/version.ts",
      "start": "ng serve",
      "start-docker": "ng serve --host 0.0.0.0 --disable-host-check",
      "clean": "npx rimraf ./dist",
      "build": "npm run clean && npm run gv && ng build",
      "watch": "ng build --watch --configuration development",
      "test": "ng test --watch=false --code-coverage",
      "test-ci": "ng test --browsers=ChromeHeadlessNoSandbox --watch=false --code-coverage",
      "lint": "ng lint",
      "lint-fix": "ng lint --fix",
      "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
    },
    "private": true,
    "dependencies": {
