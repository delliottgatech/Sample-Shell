      "@types/jasmine": "~5.1.0",
      "@typescript-eslint/eslint-plugin": "7.2.0",
      "@typescript-eslint/parser": "7.2.0",
      "copy-webpack-plugin": "^12.0.2",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-import-resolver-typescript": "^3.6.1",
      "eslint-plugin-import": "^2.29.1",
